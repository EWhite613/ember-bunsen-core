{
  "name": "ember-bunsen-core",
  "version": "0.17.1",
  "description": "Core bunsen library for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm run lint-js",
    "lint-js": "eslint addon app blueprints config tests *.js",
    "start": "ember server",
    "test": "npm run lint && ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-bunsen-core.git"
  },
  "engines": {
    "node": ">= 5.0.0"
  },
  "author": "Matthew Dahl (https://github.com/sandersky)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.5.0",
    "bunsen-core": "0.22.1",
    "ember-cli": "^2.11.0",
    "ember-cli-app-version": "^2.0.1",
    "ember-cli-chai": "0.3.2",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "0.3.6",
    "ember-cli-inject-live-reload": "^1.6.1",
    "ember-cli-mocha": "0.13.1",
    "ember-cli-moment-shim": "^3.0.1",
    "ember-cli-test-loader": "^1.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.1.1",
    "ember-load-initializers": "0.6.3",
    "ember-lodash-shim": "^1.1.1",
    "ember-resolver": "^2.1.1",
    "ember-seamless-immutable-shim": "^1.1.1",
    "ember-sinon": "0.6.0",
    "ember-validator-shim": "^2.1.1",
    "ember-z-schema": "^2.1.1",
    "eslint": "^3.14.0",
    "eslint-config-frost-standard": "^5.3.2",
    "loader.js": "^4.1.0",
    "lodash-es": "4.17.4",
    "seamless-immutable": "6.3.0",
    "validator": "6.2.1",
    "z-schema": "3.18.2"
  },
  "keywords": [
    "ember-addon",
    "bunsen",
    "bunsen-core",
    "core"
  ],
  "dependencies": {
    "broccoli-merge-trees": "^1.0.0",
    "ember-cli-babel": "^5.1.6"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "ember-lodash-shim": {
    "includes": [
      "assign",
      "capitalize",
      "chain",
      "clone",
      "cloneDeep",
      "defaults",
      "difference",
      "every",
      "find",
      "forEach",
      "forIn",
      "get",
      "groupBy",
      "includes",
      "isEmpty",
      "isNumber",
      "isObject",
      "isPlainObject",
      "isString",
      "last",
      "map",
      "mapKeys",
      "mapValues",
      "partial",
      "pick",
      "pickBy",
      "reduce",
      "size",
      "some",
      "startCase",
      "transform",
      "uniq",
      "without"
    ],
    "includeLodash3Exports": false,
    "whitelist": true
  }
}